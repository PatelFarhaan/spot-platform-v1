# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Application Configuration
CLIENT_APP_PORT=8080

# Auto-Scaler Configuration
min_od_instances=0
min_spot_instances=1
cpu_scale_up_threshold=85
cpu_scale_down_threshold=70
memory_scale_up_threshold=85
memory_scale_down_threshold=70
environment=staging
application=your-app-name
prometheus_url=http://your-prometheus-host:9090

# Slack Notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/TXXXXXXXXXX/BXXXXXXXXXX/XXXXXXXXXXXXXXXXXXXXXXXX
